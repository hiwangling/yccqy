<form  id="formreserve">
    <div class="box">
      <div class="box-body">
        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label>服务名称</label>
              <input class="form-control" name="name" placeholder="请输入服务名称" type="text">
            </div>
          </div>
		  
		  <div class="col-md-6">
            <div class="form-group">
              <label>销售价格</label>
			  <span></span>
              <input class="form-control" name="price" placeholder="请输入价格" type="text">
            </div>
          </div>
		  <div class="col-md-6">
            <div class="form-group">
              <label>相关服务</label>
              <span></span>
            <div class="input-group input-group-sm">
                <input type="text" class="form-control"  readonly="true" id="servicename" name="servicename">
                  <input type="hidden" name="service" id="service"  />
                  <span class="input-group-btn">
                      <button type="button"  onclick="select_servcieinfo();" class="btn btn-info btn-flat" >选择..</button>
                    </span>
              </div>
            </div>
          </div>  
		  
		  
		  <div class="col-md-6">
            <div class="form-group">
              <label>是否可用</label>
              <select name="status" class="form-control">
			  		<option value="1">是</option>
			        <option value="0">否</option>
              </select>
            </div>
          </div>
		  
		   <div class="col-md-6">
            <div class="form-group">
              <label>排序</label>
			  <span></span>
              <input class="form-control" name="iorder" placeholder="排序" value="0" type="text">
            </div>
          </div>
		  
		  <div class="col-md-12">
            <div class="form-group">
              <label>服务描述</label>
			  <textarea class="form-control" name="vcnote" rows="3" placeholder="请输入服务描述"></textarea>
            </div>
          </div>
        </div>
		
      </div></form>

       <div class="modal-footer"  >
        <a   href="javascript:" onclick="ComboAdd_ajax_submit();"  type="button"  class="btn btn-default"><span class="ladda-label"><i class="fa fa-check"></i> 提交</span></a>
        <a type="button" class="btn btn-default" data-dismiss="modal"> <span class="ladda-label"><i class="fa fa-close"></i> 关闭</span></a>
      </div>
    </div>
 
